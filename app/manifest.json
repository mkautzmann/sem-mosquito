{
  "name": "SemMosquito",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "Livre-se dos mosquitos nos sites .gov do Brasil",
  "icons": {
    "16": "data/images/icon-16.png",
    "128": "data/images/icon-128.png"
  },
  "default_locale": "pt_BR",
  "background": {},
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["*://*.gov.br/*"],
      "css": ["data/styles/semMosquito.css"],
      "js": ["data/scripts/semMosquito.js"]
    }
  ]
}
